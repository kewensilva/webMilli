{
    "version": 2,
    "builds": [
        {
            "src": "/src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js",
            "src": "src/public/uploads/(.*)",
            "dest": "src/public/uploads/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "rewrites": [
                {
                    "source": "/(.*)",
                    "destination": "/"
                }
            ]
        }
    ]
}