{
    "version": 2,
    "builds": [
        {
            "src": "./src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/uploads/(.*)",
            "dest": "/src/public/uploads/*"
        },
        {
            "handle": "filesystem"
        }
    ]
}