{
    "version": 2,
    "builds": [
        {
            "src": "database.sqlite",
            "use": "@vercel/static"
        },
        {
            "src": ".sequelizerc",
            "use": "@vercel/static"
        },
        {
            "src": "package.json",
            "use": "@vercel/static"
        },
        {
            "src": "/src/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "/src/*",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "^/src/(.*)$",
            "destination": "/src/server.js"
        }
    ]
}